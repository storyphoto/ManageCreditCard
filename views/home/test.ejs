    <script>
        window.onload = function() {
                var result = '<table border = 1>';
                for(var i  = 0; i < 10 ; i++){
                       result += '<tr >';
                       for(var j = 1; j < 10; j++){
                               result += '<th>';
                              
                               if(i == 0  ){
                                      result += j + 'ë‹¨';
                               } else {
                                      result += j + ' * ' + i + ' = ' + (j * i);
                               }
                               result += '</th>'
                       }
                       result += '</tr>';
                }
                result += '</table>';
                  document.body.innerHTML =  document.body.innerHTML + result;
                  
        };
 </script>

  


